{% extends 'frontbase.html.twig' %}

{% block title %}Profil Bilgileri {% endblock %}

{% block sidebar %} {{ include ('home/_usersidebar.html.twig') }} {% endblock %}
{% block body %}
<div class="col-sm-9 padding-right">
    <div class="product-details"><!--product-details-->
        <div class="col-sm-5">
            <div class="view-product">
                <h2>Profil Bilgileri</h2>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ app.user.id }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ app.user.email }}</td>
        </tr>
        <tr>
            <th>Roles</th>
            <td>{{ app.user.roles|join }}</td>
        </tr>

        <tr>
            <th>Name</th>
            <td>{{ app.user.name }}</td>
        </tr>
        <tr>
            <th>Adres</th>
            <td>{{ app.user.address }}</td>
        </tr>
        <tr>
            <th>Åžehir</th>
            <td>{{ app.user.city }}</td>
        </tr>
        <tr>
            <th>Telefon</th>
            <td>{{ app.user.phone }}</td>
        </tr>
        <tr>
            <th>Status</th>
            <td>{{ app.user.status|join }}</td>
        </tr>
        </tbody>
    </table>




            </div> </div></div></div></div>

{% endblock %}
